{
    "name": "codedor/filament-architect",
    "description": "Extended Builder field for Filament",
    "keywords": [
        "codedor",
        "laravel",
        "filament-architect"
    ],
    "homepage": "https://github.com/codedor/filament-architect",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "awcodes/drop-in-action": "^0.2.2",
        "awcodes/filament-oembed": "^1.1",
        "awcodes/filament-tiptap-editor": "^2.6",
        "codedor/filament-link-picker": "dev-master",
        "codedor/filament-media-library": "dev-master",
        "filament/filament": "^2.17",
        "illuminate/contracts": "^10.0",
        "spatie/laravel-package-tools": "^1.12"
    },
    "require-dev": {
        "codedor/facade-docblock-generator": "^0.0.1",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.0",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/pest-plugin-snapshots": "^2.0",
        "tightenco/duster": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Codedor\\FilamentArchitect\\": "src",
            "Codedor\\FilamentArchitect\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codedor\\FilamentArchitect\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codedor\\FilamentArchitect\\Providers\\FilamentArchitectServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://codedor.repo.repman.codedor.online"
        }
    ]
}
